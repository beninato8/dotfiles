args_to_list() {
    local args
    args=("$@")  # Store all arguments in an array
    local formatted_args=()

    for arg in "${args[@]}"; do
        formatted_args+=("\"$arg\",")  # Surround each argument with double quotes
    done

    local output="${formatted_args[@]}"
    output="${output:0:-1}"  # Remove the last two characters

    echo "$output"
}

