gitAddCommitPush()
{
    if ((${#*[@]} > 1)); then
        git add "$1"
        gc ${@:2}
        git push
        commits_out
    else
        echo "Not enough arguments in \"$@\""
    fi
}
alias gg='gitAddCommitPush'

gitRebaseOnto()
{
    current_branch=$(git branch --show-current)
    target_branch=${1:-master}
    git fetch origin || return 1
    BASE=$(git merge-base "$current_branch" "origin/$target_branch") || return 1
    git rebase --onto "origin/$target_branch" "$BASE" "$current_branch" || return 1
}
alias gro="gitRebaseOnto"

gitBranchDelete()
{
    [ -z "$1" ] && { echo "usage: gitDelete <branch>"; return 1; }
    git branch -D "$1" || return 1
    git push origin --delete "$1" || return 1
}
alias gbd="gitBranchDelete"

