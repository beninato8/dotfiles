bruteForceCesar()
{
    IN="$(echo "$@"|awk '{print toupper($0)}')"
    IN="$(echo $IN | tr -d '\n')"
    echo $IN
    for I in $(seq 25); do
        echo $I $IN | tr $(printf %${I}s | tr ' ' '.')\A-Z A-ZA-Z
    done
}
